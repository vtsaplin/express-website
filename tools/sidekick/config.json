{
  "project": "Express",
  "outerHost": "express-website--adobe.hlx.live",
  "host": "www.adobe.com",
  "byocdn": true,
  "pushDownSelector": "#feds-header",
  "devMode": true,
  "plugins": [
    {
      "id": "metadata",
      "title": "Meta Data Inspector",
      "url": "https://main--express-website--adobe.hlx.page/tools/metadata/inspector.html",
      "environments": [ "edit" ],
      "excludePaths": [ "/**" ],
      "includePaths": [ "**metadata.xlsx**" ]
    },
    {
      "id": "templates",
      "title": "Templates",
      "url": "https://main--express-website--adobe.hlx.page/tools/templates/picker.html",
      "environments": [ "edit" ],
      "excludePaths": [ "/**" ],
      "includePaths": [ "**.docx**" ]
    },
    {
      "id": "experiments-local",
      "title": "Experiments (Local)",
      "url": "http://localhost:3000",
      "passConfig": true,
      "passReferrer": true,
      "isPalette": true,
      "paletteRect": "position: absolute; left: 50%; top: 50%; transform: translate(-50%, -50%); width: 800px; height: 400px; box-shadow: 0 0 30px 30px rgba(0,0,0,0.15);",
      "environments": [ "dev", "preview", "live", "prod" ],
      "includePaths": [ "/**" ]
    },
    {
      "id": "experiments-azure",
      "title": "Experiments (Azure)",
      "url": "https://gentle-glacier-0d85da80f-preview.eastus2.3.azurestaticapps.net",
      "passConfig": true,
      "passReferrer": true,
      "isPalette": true,
      "paletteRect": "width: 60%; height: 60%; top: 20%; left: 20%; box-shadow: 0 0 30px 30px rgba(0,0,0,0.15);",
      "environments": [ "dev", "preview", "live", "prod" ],
      "includePaths": [ "/**" ]
    }
  ]
}
